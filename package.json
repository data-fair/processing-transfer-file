{
  "name": "@data-fair/processing-transfer-file",
  "version": "0.9.0-beta2",
  "description": "Create datasets from a simple file download, supports multiple protocols.",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "build-types": "export NODE_OPTIONS='--experimental-strip-types' && df-build-types ./",
    "test-deps": "docker compose -f test-it/docker-compose.yml up -d",
    "stop-test-deps": "docker compose -f test-it/docker-compose.yml stop",
    "test-base": "NODE_ENV=test node --experimental-strip-types --test-force-exit --test-concurrency=1 --test --test-reporter=spec --test-reporter-destination=stdout",
    "test": "npm run test-base test-it/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/data-fair/processing-transfer-file.git"
  },
  "keywords": [
    "data-fair-processings-plugin"
  ],
  "author": "Alban Mouton <alban.mouton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/data-fair/processing-transfer-file/issues"
  },
  "homepage": "https://github.com/data-fair/processing-transfer-file#readme",
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@data-fair/lib-common-types": "^1.19.4",
    "@data-fair/lib-processing-dev": "^0.2.0",
    "@data-fair/lib-types-builder": "^1.11.3",
    "@types/config": "^3.3.5",
    "@types/fs-extra": "^11.0.4",
    "@types/ftp": "^0.3.36",
    "@types/node": "^25.2.0",
    "@types/ssh2-sftp-client": "^9.0.6",
    "config": "^4.2.0",
    "debug": "^4.4.3",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "neostandard": "^0.12.2",
    "nock": "^14.0.10",
    "typescript": "^5.9.3",
    "ws": "^8.19.0"
  },
  "dependencies": {
    "@data-fair/lib-utils": "^1.9.0",
    "form-data": "^4.0.0",
    "fs-extra": "^9.1.0",
    "ftp": "^0.3.10",
    "ssh2-sftp-client": "^12.0.1"
  },
  "files": [
    "plugin-config-schema.json",
    "processing-config-schema.json",
    "./index.ts",
    "./lib/**/*"
  ]
}
